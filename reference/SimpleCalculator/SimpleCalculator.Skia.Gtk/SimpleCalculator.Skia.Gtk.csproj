<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType Condition="'$(Configuration)'=='Release'">WinExe</OutputType>
    <OutputType Condition="'$(Configuration)'=='Debug'">Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup Condition="exists('..\SimpleCalculator.Windows')">
    <EmbeddedResource Include="..\SimpleCalculator.Windows\Package.appxmanifest" LogicalName="Package.appxmanifest" />
    <Content Include="..\SimpleCalculator.Windows\Images\StoreLogo.png" Link="Assets\StoreLogo.png" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Assets\**" />
  </ItemGroup>
  <ItemGroup>
    <UpToDateCheckInput Include="..\SimpleCalculator.Shared\**\*.xaml" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="7.0.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="5.0.0" />
    <PackageReference Include="Uno.Extensions.Reactive.WinUI" Version="2.3.0-dev.263" />
    <PackageReference Include="Uno.Material.WinUI" Version="2.4.0-dev.64" />
    <PackageReference Include="Uno.Material.WinUI.Markup" Version="2.4.0-dev.64" />
    <PackageReference Include="Uno.Toolkit.WinUI.Markup" Version="2.4.0-dev.115" />
    <PackageReference Include="Uno.Toolkit.WinUI.Material" Version="2.4.0-dev.114" />
    <PackageReference Include="Uno.WinUI.Markup" Version="4.6.0-dev.13" />
    <PackageReference Include="Uno.WinUI.Skia.Gtk" Version="4.7.0-dev.158" />
    <PackageReference Include="Uno.WinUI.RemoteControl" Version="4.7.0-dev.158" Condition="'$(Configuration)'=='Debug'" />
    <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="4.7.0-dev.158" />
    <PackageReference Include="Uno.WinUI.Lottie" Version="4.7.0-dev.158" />
    <PackageReference Include="SkiaSharp.Views.Uno.WinUI" Version="2.88.3" />
    <PackageReference Include="SkiaSharp.Skottie" Version="2.88.3" />
  </ItemGroup>
  <Import Project="..\SimpleCalculator.Shared\SimpleCalculator.Shared.projitems" Label="Shared" />
</Project>