<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<DefineConstants>$(DefinedConstants);WINUI</DefineConstants>
		<!-- Ignore for SkiaSharp warning for netstandard2.0 -->
		<NoWarn>$(NoWarn);NU1701</NoWarn>
		<RootNamespace>Commerce</RootNamespace>
	</PropertyGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.Extensions.Logging" Version="6.0.0" />
		<PackageReference Include="Microsoft.Extensions.Logging.Console" Version="6.0.0" />
		<PackageReference Include="Uno.WinUI.Skia.Wpf" Version="4.4.13" />
		<PackageReference Include="Uno.WinUI.RemoteControl" Version="4.4.13" Condition="'$(Configuration)'=='Debug'" />
		<PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="4.4.13" />
		<PackageReference Include="Uno.Toolkit.WinUI" Version="2.2.0" />
		<PackageReference Include="Uno.Toolkit.WinUI.Material" Version="2.2.0" />
		<PackageReference Include="Uno.Core" Version="4.0.1" />
		<PackageReference Include="Uno.Material.WinUI" Version="2.2.0" />
		<PackageReference Include="Uno.SourceGenerationTasks" Version="4.1.0" />
	</ItemGroup>
	<ItemGroup>
		<UpToDateCheckInput Include="..\Commerce.UI\**\*.xaml" />
	</ItemGroup>
	<Import Project="..\Extensions.winui.props" />
	<ItemGroup>
		<ProjectReference Include="..\Commerce\Commerce.csproj" />
	</ItemGroup>

	<Import Project="..\Commerce.UI\Commerce.UI.projitems" Label="Shared" />

</Project>
