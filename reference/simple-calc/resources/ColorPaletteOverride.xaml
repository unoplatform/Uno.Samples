<ResourceDictionary 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation">
    <!--Colors-->
    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <Color x:Key="BackgroundColor">#F2F2F7</Color>
            <Color x:Key="PrimaryVariantDarkColor">#4B5EFC</Color>
            <Color x:Key="OnSurfaceVariantColor">#8A8494</Color>
            <Color x:Key="SecondaryContainerColor">#FFFFFF</Color>
            <Color x:Key="OnSecondaryContainerColor">#661B192C</Color>
            <Color x:Key="PrimaryContainerColor">#D2D3DA</Color>
            <Color x:Key="SurfaceColor">#E5E5EA</Color>
            <Color x:Key="PrimaryColor">#28CD55</Color>
            <Color x:Key="OnPrimaryContainerColor">#1B192C</Color>
            <Color x:Key="PrimaryVariantLightColor">#007AFF</Color>
            <Color x:Key="SecondaryColor">#D2D3DA</Color>
            <Color x:Key="SurfaceVariantColor">#F2EFF5</Color>
            <Color x:Key="TertiaryColor">#FF9500</Color>
        </ResourceDictionary>
        <ResourceDictionary x:Key="Dark">
            <Color x:Key="TertiaryColor">#FF9F0A</Color>
            <Color x:Key="PrimaryColor">#32D74B</Color>
            <Color x:Key="PrimaryContainerColor">#4E505F</Color>
            <Color x:Key="PrimaryVariantLightColor">#0A84FF</Color>
            <Color x:Key="PrimaryVariantDarkColor">#4B5EFC</Color>
            <Color x:Key="SecondaryColor">#5C5659</Color>
            <Color x:Key="SecondaryContainerColor">#2E2F38</Color>
            <Color x:Key="OnSecondaryContainerColor">#66FFFFFF</Color>
            <Color x:Key="BackgroundColor">#17171C</Color>
            <Color x:Key="OnBackgroundColor">#FFFFFF</Color>
            <Color x:Key="SurfaceColor">#777274</Color>
        </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>