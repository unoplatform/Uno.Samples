<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0-ios</TargetFramework>
    <OutputType>Exe</OutputType>
    <Nullable>enable</Nullable>
    <ImplicitUsings>true</ImplicitUsings>
    <SupportedOSPlatformVersion>13.0</SupportedOSPlatformVersion>

    <!--
      Enable full trimming in Release mode.
      To learn more, see: https://learn.microsoft.com/dotnet/core/deploying/trimming/trimming-options#trimming-granularity
    -->
    <TrimMode Condition="'$(Configuration)' == 'Release'">full</TrimMode>
    <CodesignKey>Apple Development: Created via API (SSYKXZY5Z7)</CodesignKey>
    <CodesignProvision>VS: WildCard Development</CodesignProvision>

    <MtouchUseLlvm>false</MtouchUseLlvm>
    <UseInterpreter>true</UseInterpreter>
    <MtouchInterpreter>all</MtouchInterpreter>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Uno.WinRT" Version="6.0.465" />
  </ItemGroup>
</Project>
