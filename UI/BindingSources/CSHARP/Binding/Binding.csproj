<Project Sdk="Uno.Sdk">
  <PropertyGroup>
	<TargetFrameworks>net8.0-android;net8.0-ios;net8.0-maccatalyst;net8.0-windows10.0.19041;net8.0-desktop;net8.0-browserwasm;</TargetFrameworks>

	<OutputType>Exe</OutputType>
	<UnoSingleProject>true</UnoSingleProject>

	<!-- Display name -->
	<ApplicationTitle>Binding</ApplicationTitle>
	<!-- App Identifier -->
	<ApplicationId>com.companyname.Binding</ApplicationId>
	<!-- Versions -->
	<ApplicationDisplayVersion>1.0</ApplicationDisplayVersion>
	<ApplicationVersion>1</ApplicationVersion>

	<UnoFeatures>
	  CSharpMarkup;
	  Material;
	  Toolkit;
	  Mvvm;
	</UnoFeatures>
  </PropertyGroup>

</Project>
