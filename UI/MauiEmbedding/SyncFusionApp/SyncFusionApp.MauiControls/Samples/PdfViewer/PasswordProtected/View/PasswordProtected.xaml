<?xml version="1.0" encoding="utf-8" ?>
<localCore:SampleView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:localCore="using:SyncfusionApp.MauiControls.Samples.Base"
             xmlns:local="using:SyncfusionApp.MauiControls.Samples.PdfViewer.SfPdfViewer"
             xmlns:syncfusion="clr-namespace:Syncfusion.Maui.PdfViewer;assembly=Syncfusion.Maui.PdfViewer"        
             x:Class="SyncfusionApp.MauiControls.Samples.PdfViewer.SfPdfViewer.PasswordProtected">
    <localCore:SampleView.BindingContext>
        <local:PasswordProtectedViewModel/>
    </localCore:SampleView.BindingContext>
    <localCore:SampleView.Content>
        <Grid>
            <ContentView Content="{Binding Content}" />
        </Grid>
    </localCore:SampleView.Content>
</localCore:SampleView>