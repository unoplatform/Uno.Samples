<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType Condition="'$(Configuration)'=='Release'">WinExe</OutputType>
    <OutputType Condition="'$(Configuration)'=='Debug'">Exe</OutputType>
    <TargetFramework>net7.0</TargetFramework>
  </PropertyGroup>
  <ItemGroup Condition="exists('..\Uno.Sample.ControlLibrary.Windows')">
    <EmbeddedResource Include="..\Uno.Sample.ControlLibrary.Windows\Package.appxmanifest" LogicalName="Package.appxmanifest" />
    <Content Include="..\Uno.Sample.ControlLibrary.Windows\Images\StoreLogo.png" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Console" Version="7.0.0" />
    <PackageReference Include="Uno.WinUI.Skia.Gtk" Version="4.9.26" />
    <PackageReference Include="Uno.WinUI.RemoteControl" Version="4.9.26" Condition="'$(Configuration)'=='Debug'" />
    <PackageReference Include="Uno.UI.Adapter.Microsoft.Extensions.Logging" Version="4.9.26" />
    <PackageReference Include="Uno.WinUI.Lottie" Version="4.9.26" />
    <PackageReference Include="SkiaSharp.Views.Uno.WinUI" Version="2.88.4-preview.84" />
    <PackageReference Include="SkiaSharp.Skottie" Version="2.88.4-preview.84" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Uno.Sample.ControlLibrary\Uno.Sample.ControlLibrary.csproj" />
  </ItemGroup>
  <Import Project="..\Uno.Sample.ControlLibrary.Base\base.props" />
</Project>